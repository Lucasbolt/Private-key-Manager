{
  "name": "private-key-manager",
  "version": "2.0.1",
  "author": "Chisom Ibekwe <lucasbolt700@gmail.com>",
  "license": "ISC",
  "description": "A CLI tool for managing private keys securely",
  "keywords": [
    "cli",
    "private-key",
    "key-manager",
    "security",
    "passphrase"
  ],
  "main": "./buld/src/cli/index.js",
  "bin": {
    "pkm-cli": "build/src/cli/index.js"
  },
  "files": [
    "build/**/*",
    "README.md",
    "LICENSE"
  ],
  "type": "module",
  "scripts": {
    "start": "NODE_ENV=production src/cli/index.js",
    "build": "node scripts/syncVersion.js && tsc && tsc-alias",
    "lint": "eslint .",
    "prepublishOnly": "npm run lint && npm run build",
    "dev": "tsx ./src/cli/index.ts",
    "package-cli": "npm run build && npm unlink -g && npm link",
    "watch": "tsx watch src/index.ts",
    "test": "jest --coverage",
    "test:watch": "jest --watch"
  },
  "dependencies": {
    "aws-sdk": "^2.1692.0",
    "axios": "^1.8.4",
    "bcrypt": "^6.0.0",
    "chalk": "^5.4.1",
    "dotenv": "^16.4.7",
    "env-paths": "^3.0.0",
    "googleapis": "^146.0.0",
    "inquirer": "^12.4.3",
    "level": "^9.0.0",
    "open": "^10.1.0",
    "winston": "^3.17.0",
    "winston-daily-rotate-file": "^5.0.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.25.1",
    "@types/bcrypt": "^5.0.2",
    "@types/figlet": "^1.7.0",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.13.10",
    "@types/supertest": "^6.0.2",
    "dotenv-cli": "^8.0.0",
    "eslint": "^9.25.1",
    "eslint-plugin-import": "^2.31.0",
    "globals": "^16.0.0",
    "jest": "^29.7.0",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.6",
    "ts-node": "^10.9.2",
    "tsc-alias": "^1.8.15",
    "tsx": "^4.19.3",
    "typescript": "^5.8.2",
    "typescript-eslint": "^8.31.1"
  },
  "engines": {
    "node": ">=18"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Lucasbolt/Private-key-Manager"
  }
}
