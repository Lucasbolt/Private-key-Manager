{
  "name": "private-key-manager",
  "version": "1.0.1",
  "author": "Chisom Ibekwe <lucasbolt700@gmail.com>",
  "license": "ISC",
  "description": "A CLI tool and desktop app for managing private keys securely",
  "keywords": [
    "cli",
    "private-key",
    "key-manager",
    "security",
    "passphrase"
  ],
  "main": "./buld/src/cli/index.js",
  "bin": {
    "pkm-cli": "build/src/cli/index.js"
  },
  "files": [
    "build/**/*",
    "README.md",
    "LICENSE"
  ],
  "type": "module",
  "scripts": {
    "start": "NODE_ENV=production TS_NODE_BASEURL=./build node -r tsconfig-paths/register build/src/cli/index.js",
    "build": "tsc && tsc-alias",
    "prepublishOnly": "npm run build",
    "dev": "tsx",
    "package-cli": "npm run build && npm unlink -g && npm link",
    "watch": "tsx watch src/index.ts",
    "test": "jest --coverage",
    "test:watch": "jest --watch"
  },
  "dependencies": {
    "@types/node": "^22.13.10",
    "aws-sdk": "^2.1692.0",
    "axios": "^1.8.4",
    "bcrypt": "^5.1.1",
    "chalk": "^5.4.1",
    "commander": "^13.1.0",
    "dotenv": "^16.4.7",
    "env-paths": "^3.0.0",
    "figlet": "^1.8.0",
    "googleapis": "^146.0.0",
    "inquirer": "^12.4.3",
    "level": "^9.0.0",
    "module-alias": "^2.2.3",
    "open": "^10.1.0",
    "react-router-dom": "^7.5.0",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.8.2",
    "winston": "^3.17.0",
    "winston-daily-rotate-file": "^5.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.27.0",
    "@babel/preset-env": "^7.26.9",
    "@babel/preset-typescript": "^7.27.0",
    "@types/bcrypt": "^5.0.2",
    "@types/figlet": "^1.7.0",
    "@types/jest": "^29.5.14",
    "@types/supertest": "^6.0.2",
    "babel-plugin-module-resolver": "^5.0.2",
    "babel-plugin-tsconfig-paths": "^1.0.3",
    "dotenv-cli": "^8.0.0",
    "jest": "^29.7.0",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.6",
    "tsc-alias": "^1.8.15",
    "tsx": "^4.19.3"
  },
  "_moduleAliases": {
    "@root": ".",
    "@src": "src",
    "@tests": "test",
    "@services": "src/services",
    "@utils": "src/utils",
    "@commands": "src/commands"
  }
}
